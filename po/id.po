#. TRANSLATORS: ascli flag description for: --pedantic
msgid "Also print pedantic hints when validating"
msgstr "Cetak juga saran pedantik ketika memvalidasi"

msgid "An item type (e.g. lib, bin, python3, ...)"
msgstr "Suatu tipe butir (mis. lib, bin, python3, ...)"

#. TRANSLATORS: Output if appstreamcli --version is executed.
#, c-format
msgid "AppStream CLI tool version: %s"
msgstr "Alat CLI AppStream versi: %s"

#. TRANSLATORS: In the status report of ascli: Header
msgid "AppStream Status:"
msgstr "Status AppStream:"

#. TRANSLATORS: Updating the metadata cache succeeded
msgid "AppStream cache update completed successfully."
msgstr "Pemutakhiran singgahan AppStream berakhir dengan sukses."

#, c-format
msgid "AppStream cache update failed."
msgstr "Pemutakhiran singgahan AppStream gagal."

msgid "AppStream cache update is not necessary."
msgstr "Pemutakhiran singgahan AppStream tidak perlu."

#. TRANSLATORS: This is the header to the --help menu
msgid "AppStream command-line interface"
msgstr "Antar muka baris perintah AppStream"

#, c-format
msgid ""
"AppStream data pool was loaded, but some metadata was ignored due to errors."
msgstr ""
"Pool data AppStream dimuat, tapi beberapa metadata diabaikan karena "
"kesalahan."

msgid "Binaries"
msgstr "Biner"

msgid "Bundle"
msgstr "Bundel"

#, c-format
msgid "Cache location '%s' is not writable."
msgstr "Lokasi singgahan '%s' tidak dapat ditulisi."

#, c-format
msgid ""
"Can not copy '%s': File does not have a '.metainfo.xml' or '.appdata.xml' "
"suffix."
msgstr ""
"Tidak bisa menyalin '%s': Berkas tidak memiliki akhiran '.metainfo.xml' atau "
"'.appdata.xml'."

msgid "Can not search for unknown component type."
msgstr "Tidak bisa mencari tipe komponen yang tak dikenal."

msgid "Categories"
msgstr "Kategori"

#. TRANSLATORS: ascli has been run with unknown command.
#, c-format
msgid "Command '%s' is unknown."
msgstr "Perintah '%s' tak dikenal."

#, c-format
msgid "Component '%s' has no installation candidate."
msgstr "Komponen '%s' tidak punya kandidat instalasi."

msgid "Compulsory for"
msgstr "Wajib bagi"

